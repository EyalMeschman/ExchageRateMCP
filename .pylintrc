[FORMAT]
max-line-length=140
disable=missing-function-docstring, missing-module-docstring, missing-class-docstring, logging-fstring-interpolation, broad-exception-raised, too-many-arguments, too-few-public-methods, too-many-locals, R0917

[SIMILARITIES]
min-similarity-lines=4
ignore-comments=yes
ignore-docstrings=yes
ignore-imports=yes

[DESIGN]
max-attributes=10
